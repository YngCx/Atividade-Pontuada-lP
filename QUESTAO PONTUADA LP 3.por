programa {
  funcao inicio() {
    
      //Declarar variavel

      inteiro a, b
      real c

      // iNFORMAÇOES
      
      escreva ("DIGITE O VALOR DE A: ")
      leia(a)

      escreva ("DIGITE O VALOR DE B: ")
      leia(b)

      //calcule

      se (a == b) { 
        c = a + b
      }senao{
        c = a*b 
      }
      escreva ("O RESULTALDO FINAL DOS VALORES C: ", c)
      







  }
}
