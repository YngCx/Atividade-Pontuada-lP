programa {
  funcao inicio() {


    //Declarar Variaveis
    real a1, b2, c3, soma

    
    //Escreva os valores 
    escreva ("Escreva o valor de A: ")
    leia(a1)

    escreva ("Escreva o valor de B: ")
    leia(b2)

    escreva ("Escreva o valor de C: ")
    leia(c3)

    // Calcule os valores 


    soma = a1+b2
    se (soma < c3){
      escreva ("O Valor da soma é menor que c3")

    }senao {
      escreva ("O Valor da soma é maior que c3")
    }

  }


  }
